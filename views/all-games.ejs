<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h1>These are all your games</h1>

    <ul>
        <% games.forEach((aGame) => { %>
            <li>
                <a href="/games/<%=aGame._id%>/detail" method="get"><%= aGame.gameName %></a>
            </li>
            <form action="/games/<%=aGame._id%>/delete" method="post">
              <button>Delete</button>
            </form>
            <form action="/games/<%=aGame._id%>/"></form>
            
            <% }) %>
    </ul>

    <a href="/">Go Back</a>
  </body>
</html>
